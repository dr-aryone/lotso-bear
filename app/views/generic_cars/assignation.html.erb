
	<h1>Carro genérico | Asignación de Familias </h1>
	<%= form_for @generic_car do |builder| %>

		<%= render 'shared/generic_car', car: @generic_car%>

		<%= render 'generic_car_families', car: @generic_car, f: builder %>

		<%= render 'other_generic_car_families', car: @other_families %>

		<%= link_to "Cancel", generic_cars_path , class: "btn " %>

		<%= submit_tag "Guardar Coche", class: "btn btn-primary" %>

	<% end %>
