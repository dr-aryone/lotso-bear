<%= form_for(@warehouse) do |f| %>
  <%= render 'shared/error_messages', model: @warehouse %>
  <div class="field">
    <%= f.label :branch, "Nombre" %><br>
    <%= f.text_field :branch,:required=>"true" %>
  </div>

  <div class="field">
    <%= f.label :location, "Ubicación" %><br>
    <%= f.text_field :location,:required=>"true" %>
  </div>

  <div class="field">
    <%= f.label :code, "Código" %><br>
    <%= f.text_field :code,:required=>"true" %>
  </div>

  <div class="field">
    <%= f.label :geolocation, "Geolocalización" %><br>
    <%= f.text_field :geolocation,:required=>"true" %>
  </div>

  <div class="actions">
    <%= f.submit "Guardar Almacén", class:"btn btn-primary" %>
  </div>
<% end %>
