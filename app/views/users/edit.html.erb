<h1>Dar de alta usuario</h1>
<p>Ingresa los datos del usuario</p>

<div class="row">
  <div class="span6 offset3">

    <%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages', model: @user %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :clock_id, "ID Del Reloj Checador" %>
      <%= f.text_field :clock_id %>


      <%= f.label :privileges %>
      <%= f.select(:privileges, ApplicationHelper::PRIVS) %>

      <%= f.submit "Dar de Alta Usuario", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
