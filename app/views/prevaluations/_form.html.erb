<%= form_for @prevaluation, :html => {:multipart=>true} do |prevaluation|%>
  <%= render "prevaluation_fields", builder: prevaluation %>

    <%= prevaluation.fields_for :cars do |car| %>
      <%= render "car_fields", builder: car %>
    <% end %>

    <%= prevaluation.fields_for :insureds do |insured| %>
      <%= render "insured_fields", builder: insured %>
    <% end %>

    <%= prevaluation.fields_for :insureds do |insured| %>
      <%= render "third_insured_fields", builder: insured %>
    <% end %>


    <%= render "attachment_fields", builder: prevaluation %>

  <%= prevaluation.submit "Guardar" %>
<% end %>
